class Program
{
    static void Main()
    {
        Console.WriteLine("Digite um número");
        int entrada1 = Convert.ToInt32(Console.ReadLine());
        
        string resultado = (entrada1 % 2 == 0) ? "par" : "ímpar";  
        Console.WriteLine("O número é " +  resultado);  

        Console.ReadKey();  
    }
}
