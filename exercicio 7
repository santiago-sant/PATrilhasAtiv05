using System;

class Program
{
    static void Main()
    {
        Console.Write("Digite um número: ");
        int entrada1 = int.Parse(Console.ReadLine());

        string resultado = (entrada1 > 0) ? "positivo" : (entrada1 < 0) ? "negativo" :  "zero";

        Console.WriteLine("O numero é " + resultado );
        Console.ReadKey();
    }
}
